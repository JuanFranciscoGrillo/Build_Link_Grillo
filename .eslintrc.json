{
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
      // Add any additional ESLint configurations here as needed
    ],
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "rules": {
        "linebreak-style": ["error", "unix"]
    },
    "ignorePatterns": [
      "node_modules/",
      "build/",
      "dist/",
      "out/",
      "db/migrations/",
      "db/seeds/",
      "*.log",
      "*.min.js",
      "*.min.css",
      "*.bundle.js",
      "*.map",
      ".eslintrc.js",
      ".prettierrc.js"
      // Add any other files or directories you want to ignore here
    ]
  }
  